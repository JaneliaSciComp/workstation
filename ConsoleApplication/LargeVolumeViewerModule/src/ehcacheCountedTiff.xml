<ehcache 
    xsi:noNamespaceSchemaLocation="ehcache.xsd"
    updateCheck="false"
    monitoring="off" 
    dynamicConfig="true">
    <!-- maxBytesLocalDisk="0g"    -->
    <cacheManagerEventListenerFactory class="" properties=""/>
    <transactionManagerLookup class="net.sf.ehcache.transaction.manager.DefaultTransactionManagerLookup" properties="jndiName=java:/TransactionManager" propertySeparator=";"/>
    <defaultCache maxEntriesLocalHeap="0" eternal="false" timeToIdleSeconds="0" timeToLiveSeconds="0">
    </defaultCache>
    <sizeOfPolicy maxDepth="10000" maxDepthExceededBehavior="continue"/>
    <!-- NOTE: I have two of these, same name.  Assuming one at a time. -->
    <!-- Setting up max entries local heap to a small enough value for anything
         I can currently anticipate. -->
    <cache name="CompressedTiffCache"
           timeToIdleSeconds="0" 
           timeToLiveSeconds="0" 
           memoryStoreEvictionPolicy="LRU"
           eternal="false"
           overflowToOffHeap="false"
           maxEntriesLocalHeap="90"
    >
        <cacheEventListenerFactory class="org.janelia.it.workstation.cache.large_volume.listener.LargeVolumeCacheListenerFactory" properties="" listenFor="all"/>
    </cache>
</ehcache>
